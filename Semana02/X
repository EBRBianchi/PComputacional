for x in range(5):
    for y in range(5):
        if x == y and y == 0:
            print("*           * ", end="")
        elif x == y and y == 1:
            print("   *     *       ", end="")
        elif x == y and y == 2:
            print("      *       ", end="")
        elif x == y and y == 3:
            print("   *     *    ", end="")
        elif x == y and y == 4:
            print("*           * ", end="")   
    print("")
