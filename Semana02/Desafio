for x in range(7):
    N = x + 1
    ultimo = None
    penultimo = None
    valor = 1

for i in range(1,N+1):
    if i == 1 or i == 2:
        penultimo = ultimo
        ultimo = 1
    else:
        valor = ultimo + penultimo
        penultimo = ultimo
        ultimo = valor
    for y in range(valor + 1):
        print("* ", end=" ")
    print("")
