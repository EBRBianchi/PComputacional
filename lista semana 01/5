lista_A = []
lista_B = []
lista_C = []
posição = 0
elemento = 0
elementoB = 0
count = 0
countB = 0
countC = 0
while countC < 5:
    elemento = int(input("Digite seu elemento:"))
    lista_A.append(elemento)
    elementoB = int(input("Digite seu elemento B:"))
    lista_B.append(elementoB)
    countC += 1
    print(lista_A)
    print(lista_B)
    
    
    if elemento != elementoB:
        elementoB = lista_B[countB]
        lista_C.append(elementoB)
        countB += 1
        elemento = lista_A[count]
        lista_C.append(elemento)
        count += 1
    


print (lista_C)
